{
    "name": "coderius/php-helpers",
    "description": "Provides php helpers.",
    "keywords": ["php", "helpers"],
    "license": "MIT",
    "authors": [
        {
            "name": "coderius",
            "email": "sunrise4fun@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PhpHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    
    "minimum-stability": "dev",
    "prefer-stable": true
}
